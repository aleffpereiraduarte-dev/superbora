<script>
const CONFIG = {
    DELIVERY_FEE: 5.99,
    FREE_DELIVERY_MIN: <?= $membership_frete_minimo ?>,
    FREE_DELIVERY_ALWAYS: <?= $membership_frete_gratis ? 'true' : 'false' ?>,
    MEMBERSHIP_DISCOUNT: <?= $membership_discount ?>,
    MEMBERSHIP_LEVEL: '<?= $membership_level ?>',
    MEMBERSHIP_ICON: '<?= $membership_icon ?>'
};

let cart = <?= json_encode(array_values($cart)) ?>;

// Header scroll
window.addEventListener('scroll', () => {
    const header = document.getElementById('header');
    if (header) header.classList.toggle('scrolled', window.scrollY > 50);
});

// Timer
let timeLeft = <?= $fim_ofertas ?>;
function updateTimer() {
    if (timeLeft <= 0) return;
    timeLeft--;
    const h = Math.floor(timeLeft / 3600);
    const m = Math.floor((timeLeft % 3600) / 60);
    const s = timeLeft % 60;
    const hEl = document.getElementById('timerH');
    const mEl = document.getElementById('timerM');
    const sEl = document.getElementById('timerS');
    if (hEl) hEl.textContent = String(h).padStart(2, '0');
    if (mEl) mEl.textContent = String(m).padStart(2, '0');
    if (sEl) sEl.textContent = String(s).padStart(2, '0');
}
setInterval(updateTimer, 1000);

// Cart
function updateCartUI() {
    const items = document.getElementById('cartItems');
    const badge = document.getElementById('cartBadge');
    const subtotalEl = document.getElementById('cartSubtotal');
    const discountEl = document.getElementById('cartDiscount');
    const deliveryEl = document.getElementById('cartDelivery');
    const totalEl = document.getElementById('cartTotal');
    const checkoutBtn = document.getElementById('checkoutBtn');
    
    let count = 0, subtotal = 0;
    cart.forEach(item => {
        count += item.qty;
        const price = item.price_promo > 0 ? item.price_promo : item.price;
        subtotal += price * item.qty;
    });
    
    if (badge) badge.textContent = count;
    
    // Atualizar badge mobile tambÃ©m
    const mobileBadge = document.getElementById('mobileCartBadge');
    if (mobileBadge) mobileBadge.textContent = count;
    
    if (cart.length === 0) {
        if (items) items.innerHTML = '<div class="cart-empty"><div class="cart-empty-icon">ğŸ›’</div><h3>Carrinho vazio</h3><p>Adicione produtos para comeÃ§ar</p></div>';
        if (checkoutBtn) checkoutBtn.disabled = true;
    } else {
        if (items) items.innerHTML = '<div class="cart-items">' + cart.map(item => {
            const price = item.price_promo > 0 ? item.price_promo : item.price;
            const itemTotal = price * item.qty;
            return `
                <div class="cart-item">
                    <div class="cart-item-img">
                        <img src="${item.image || 'https://via.placeholder.com/64'}" alt="" onerror="this.src='https://via.placeholder.com/64'">
                    </div>
                    <div class="cart-item-info">
                        <div class="cart-item-name">${item.name}</div>
                        <div class="cart-item-price">R$ ${itemTotal.toFixed(2).replace('.', ',')}</div>
                        <div class="cart-item-qty">
                            <button class="qty-btn" onclick="changeQty(${item.product_id}, -1)">âˆ’</button>
                            <span class="qty-value">${item.qty}</span>
                            <button class="qty-btn" onclick="changeQty(${item.product_id}, 1)">+</button>
                        </div>
                    </div>
                </div>
            `;
        }).join('') + '</div>';
        if (checkoutBtn) checkoutBtn.disabled = false;
    }
    
    const discount = subtotal * (CONFIG.MEMBERSHIP_DISCOUNT / 100);
    const delivery = CONFIG.FREE_DELIVERY_ALWAYS || (CONFIG.FREE_DELIVERY_MIN > 0 && subtotal >= CONFIG.FREE_DELIVERY_MIN) || (CONFIG.FREE_DELIVERY_MIN === 0) ? 0 : CONFIG.DELIVERY_FEE;
    const total = subtotal - discount + delivery;
    
    if (subtotalEl) subtotalEl.textContent = 'R$ ' + subtotal.toFixed(2).replace('.', ',');
    if (discountEl) {
        discountEl.textContent = '-R$ ' + discount.toFixed(2).replace('.', ',');
        discountEl.parentElement.style.display = discount > 0 ? 'flex' : 'none';
    }
    
    if (CONFIG.FREE_DELIVERY_ALWAYS) {
        if (deliveryEl) deliveryEl.innerHTML = '<span style="color: var(--primary); font-weight: 700;">' + CONFIG.MEMBERSHIP_ICON + ' GrÃ¡tis!</span>';
    } else if (delivery === 0) {
        if (deliveryEl) deliveryEl.innerHTML = '<span style="color: var(--primary); font-weight: 700;">GrÃ¡tis!</span>';
    } else {
        const faltam = CONFIG.FREE_DELIVERY_MIN - subtotal;
        if (deliveryEl) deliveryEl.innerHTML = 'R$ ' + delivery.toFixed(2).replace('.', ',') + ' <small style="color: var(--gray-500);">(faltam R$ ' + faltam.toFixed(2).replace('.', ',') + ' p/ grÃ¡tis)</small>';
    }
    
    if (totalEl) totalEl.textContent = 'R$ ' + total.toFixed(2).replace('.', ',');
    
    saveCart();
}

// Tabs Navigation
document.querySelectorAll('.tab-btn').forEach(btn => {
    btn.addEventListener('click', () => {
        const tabId = btn.dataset.tab;
        
        // Remove active from all buttons
        document.querySelectorAll('.tab-btn').forEach(b => b.classList.remove('active'));
        btn.classList.add('active');
        
        // Hide all tab contents
        document.querySelectorAll('.tab-content').forEach(c => c.classList.remove('active'));
        
        // Show selected tab
        const content = document.getElementById('tab-' + tabId);
        if (content) content.classList.add('active');
    });
});

// Cestas data (from PHP)
const cestasData = <?= json_encode($cestas) ?>;

// Copiar cupom
function copyCupom(code) {
    navigator.clipboard.writeText(code).then(() => {
        showToast('Cupom copiado! ' + code, 'success');
    }).catch(() => {
        // Fallback
        const input = document.createElement('input');
        input.value = code;
        document.body.appendChild(input);
        input.select();
        document.execCommand('copy');
        document.body.removeChild(input);
        showToast('Cupom copiado! ' + code, 'success');
    });
}

// Adicionar combo ao carrinho
function addComboToCart() {
    const comboItems = [
        { product_id: 'combo-arroz', name: 'Arroz Tipo 1 5kg', price: 24.90, price_promo: 0, image: '', qty: 1 },
        { product_id: 'combo-feijao', name: 'FeijÃ£o Carioca 1kg', price: 8.90, price_promo: 0, image: '', qty: 1 },
        { product_id: 'combo-oleo', name: 'Ã“leo de Soja 900ml', price: 7.90, price_promo: 0, image: '', qty: 1 }
    ];
    
    // Adiciona combo como item Ãºnico com desconto
    const existing = cart.find(i => i.product_id === 'combo-basico');
    if (existing) {
        existing.qty++;
    } else {
        cart.push({
            product_id: 'combo-basico',
            name: 'Combo BÃ¡sico (Arroz + FeijÃ£o + Ã“leo)',
            price: 35.50,
            price_promo: 0,
            image: '',
            qty: 1,
            is_combo: true
        });
    }
    
    updateCartUI();
    showToast('Combo adicionado com 15% OFF!', 'success');
    openCartSidebar();
}

function addCestaToCart(cestaId) {
    const cesta = cestasData.find(c => c.id === cestaId);
    if (!cesta) {
        // Cesta bÃ¡sica hardcoded para o banner hero
        if (cestaId === 'basica') {
            const existing = cart.find(i => i.product_id === 'cesta-basica');
            if (existing) {
                existing.qty++;
            } else {
                cart.push({
                    product_id: 'cesta-basica',
                    name: 'Cesta BÃ¡sica Completa',
                    price: 149.90,
                    price_promo: 0,
                    image: '',
                    qty: 1,
                    is_cesta: true
                });
            }
            updateCartUI();
            showToast('Cesta BÃ¡sica adicionada!', 'success');
            openCartSidebar();
            return;
        }
        return;
    }
    
    // Add cesta as a special item
    const existing = cart.find(i => i.product_id === 'cesta-' + cestaId);
    
    if (existing) {
        existing.qty++;
    } else {
        cart.push({
            product_id: 'cesta-' + cestaId,
            name: cesta.nome,
            price: cesta.preco_por,
            price_promo: 0,
            image: '',
            qty: 1,
            is_cesta: true,
            itens: cesta.itens
        });
    }
    
    updateCartUI();
    showToast(cesta.nome + ' adicionada!', 'success');
    openCartSidebar();
}


function addToCart(productId, name, price, image) {
    console.log('addToCart chamado:', {productId, name, price, image});
    
    const card = document.querySelector(`[data-id="${productId}"]`);
    const addBtn = card?.querySelector('.add-btn, .quick-add-btn');
    if (addBtn) {
        addBtn.disabled = true;
        addBtn.innerHTML = '<span style="font-size:12px">...</span>';
    }
    
    fetch('/mercado/api/cart.php', {
        method: 'POST',
        headers: {'Content-Type': 'application/json'},
        body: JSON.stringify({
            action: 'add',
            product_id: productId,
            name: name || 'Produto',
            price: price || 0,
            price_promo: 0,
            image: image || '',
            qty: 1
        })
    })
    .then(r => r.json())
    .then(data => {
        console.log('Resposta API:', data);
        if (data.success) {
            const existing = cart.find(i => i.product_id === productId);
            if (existing) {
                existing.qty++;
            } else {
                cart.push({ product_id: productId, name: name || 'Produto', price, price_promo: 0, image, qty: 1 });
            }
            
            const count = data.count || cart.reduce((sum, i) => sum + i.qty, 0);
            document.querySelectorAll('#cartBadge, #mobileCartBadge, .cart-badge, .mkt-cart-count').forEach(b => {
                if (b) b.textContent = count;
            });
            
            const qtyCtrl = document.getElementById('qty-' + productId);
            if (qtyCtrl) {
                qtyCtrl.classList.add('show');
                const qtyVal = qtyCtrl.querySelector('.qty-value');
                if (qtyVal) qtyVal.textContent = existing ? existing.qty : 1;
            }
            
            if (addBtn) addBtn.style.display = 'none';
            showToast('Produto adicionado!', 'success');
        } else {
            console.error('Erro API:', data);
            showToast('Erro ao adicionar', 'error');
            resetBtn(addBtn);
        }
    })
    .catch(err => {
        console.error('Erro fetch:', err);
        showToast('Erro de conexÃ£o', 'error');
        resetBtn(addBtn);
    });
}

function resetBtn(btn) {
    if (btn) {
        btn.disabled = false;
        btn.innerHTML = '<svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><line x1="12" y1="5" x2="12" y2="19"/><line x1="5" y1="12" x2="19" y2="12"/></svg>';
    }
}

function changeQty(productId, delta) {
    const item = cart.find(i => i.product_id === productId);
    if (!item) return;
    
    const newQty = item.qty + delta;
    
    if (newQty <= 0) {
        fetch('/mercado/api/cart.php', {
            method: 'POST',
            headers: {'Content-Type': 'application/json'},
            body: JSON.stringify({action: 'remove', product_id: productId})
        }).then(r => r.json()).then(data => {
            cart = cart.filter(i => i.product_id !== productId);
            
            const qtyCtrl = document.getElementById('qty-' + productId);
            if (qtyCtrl) qtyCtrl.classList.remove('show');
            
            const card = document.querySelector(`[data-id="${productId}"]`);
            if (card) {
                const addBtn = card.querySelector('.add-btn, .quick-add-btn');
                if (addBtn) {
                    addBtn.style.display = 'flex';
                    addBtn.disabled = false;
                    addBtn.innerHTML = '<svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><line x1="12" y1="5" x2="12" y2="19"/><line x1="5" y1="12" x2="19" y2="12"/></svg>';
                }
            }
            
            updateCartUI();
        });
    } else {
        fetch('/mercado/api/cart.php', {
            method: 'POST',
            headers: {'Content-Type': 'application/json'},
            body: JSON.stringify({action: 'update', product_id: productId, qty: newQty})
        }).then(r => r.json()).then(data => {
            item.qty = newQty;
            
            const qtyCtrl = document.getElementById('qty-' + productId);
            if (qtyCtrl) {
                const qtyVal = qtyCtrl.querySelector('.qty-value');
                if (qtyVal) qtyVal.textContent = newQty;
            }
            
            updateCartUI();
        });
    }
}

function saveCart() {
    fetch('/mercado/api/cart.php', {
        method: 'POST',
        headers: { 'Content-Type': 'application/json' },
        body: JSON.stringify({ action: 'save', cart: cart.reduce((acc, i) => { acc[i.product_id] = i; return acc; }, {}) })
    }).catch(() => {});
}

function toggleCart() {
    const sidebar = document.getElementById('cartSidebar');
    const overlay = document.getElementById('cartOverlay');
    if (sidebar && overlay) {
        sidebar.classList.toggle('open');
        overlay.classList.toggle('open');
        document.body.style.overflow = sidebar.classList.contains('open') ? 'hidden' : '';
    }
}

function goToCheckout() {
    if (cart.length === 0) { showToast('Adicione produtos!', 'error'); return; }
    window.location.href = '/mercado/checkout.php';
}

// Address Modal
function openAddressModal() {
    document.getElementById('addressOverlay')?.classList.add('open');
    document.getElementById('addressModal')?.classList.add('open');
    document.body.style.overflow = 'hidden';
}

function closeAddressModal() {
    document.getElementById('addressOverlay')?.classList.remove('open');
    document.getElementById('addressModal')?.classList.remove('open');
    document.body.style.overflow = '';
}

function selectAddress(addressId) {
    fetch('/mercado/api/address.php', {
        method: 'POST',
        headers: { 'Content-Type': 'application/json' },
        body: JSON.stringify({ action: 'select', address_id: addressId })
    }).then(() => location.reload()).catch(() => location.reload());
}

// Toast
function showToast(message, type = 'success') {
    const toast = document.getElementById('toast');
    document.getElementById('toastIcon').textContent = type === 'success' ? 'âœ“' : 'âœ•';
    document.getElementById('toastText').textContent = message;
    toast.className = 'toast ' + type;
    toast.classList.add('show');
    setTimeout(() => toast.classList.remove('show'), 3000);
}

updateCartUI();
