<script>
// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
// ğŸŒŸ HEADER INTERACTIONS - WORLD CLASS
// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

(function() {
    const header = document.getElementById('omHeader');
    const userTrigger = document.getElementById('omUserTrigger');
    const userMenu = userTrigger?.closest('.om-user-menu');
    const searchInput = document.getElementById('omSearchInput');
    
    let lastScrollY = 0;
    let ticking = false;
    
    // Partially Persistent Header
    function handleScroll() {
        const currentScrollY = window.scrollY;
        
        if (currentScrollY > 100) {
            header.classList.add('scrolled');
            
            if (currentScrollY > lastScrollY && currentScrollY > 200) {
                // Scrolling down
                header.classList.add('hidden');
            } else {
                // Scrolling up
                header.classList.remove('hidden');
            }
        } else {
            header.classList.remove('scrolled', 'hidden');
        }
        
        lastScrollY = currentScrollY;
        ticking = false;
    }
    
    window.addEventListener('scroll', function() {
        if (!ticking) {
            requestAnimationFrame(handleScroll);
            ticking = true;
        }
    }, { passive: true });
    
    // User Dropdown
    if (userTrigger && userMenu) {
        userTrigger.addEventListener('click', function(e) {
            e.stopPropagation();
            userMenu.classList.toggle('open');
        });
        
        document.addEventListener('click', function(e) {
            if (!userMenu.contains(e.target)) {
                userMenu.classList.remove('open');
            }
        });
    }
    
    // Search Shortcut (Cmd/Ctrl + K)
    document.addEventListener('keydown', function(e) {
        if ((e.metaKey || e.ctrlKey) && e.key === 'k') {
            e.preventDefault();
            searchInput?.focus();
        }
        
        // ESC to blur search
        if (e.key === 'Escape' && document.activeElement === searchInput) {
            searchInput.blur();
        }
    });
    
    // Smooth Categories Scroll
    const catsScroll = document.getElementById('omCatsScroll');
    if (catsScroll) {
        let isDown = false;
        let startX;
        let scrollLeft;
        
        catsScroll.addEventListener('mousedown', (e) => {
            isDown = true;
            startX = e.pageX - catsScroll.offsetLeft;
            scrollLeft = catsScroll.scrollLeft;
        });
        
        catsScroll.addEventListener('mouseleave', () => isDown = false);
        catsScroll.addEventListener('mouseup', () => isDown = false);
        
        catsScroll.addEventListener('mousemove', (e) => {
            if (!isDown) return;
            e.preventDefault();
            const x = e.pageX - catsScroll.offsetLeft;
            const walk = (x - startX) * 2;
            catsScroll.scrollLeft = scrollLeft - walk;
        });
    }
    
    console.log('ğŸŒŸ Header World-Class initialized!');
})();
</script>

