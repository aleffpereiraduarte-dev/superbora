// DADOS DO CLIENTE LOGADO (OpenCart)
// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

$pro_is_logged = isset($_SESSION['customer_id']) && $_SESSION['customer_id'] > 0;
$pro_customer_id = $_SESSION['customer_id'] ?? 0;
$pro_customer_name = '';
$pro_customer_firstname = '';
$pro_membership_level = 'bronze';
$pro_membership_discount = 0;
$pro_membership_icon = 'ğŸ¥‰';
$pro_membership_frete_minimo = 150;

if ($pro_is_logged && isset($pdo)) {
    try {
        // Buscar dados do cliente
        $stmt = $pdo->prepare("SELECT firstname, lastname, email FROM oc_customer WHERE customer_id = ?");
        $stmt->execute([$pro_customer_id]);
        $pro_customer = $stmt->fetch(PDO::FETCH_ASSOC);
        
        if ($pro_customer) {
            $pro_customer_firstname = $pro_customer['firstname'];
            $pro_customer_name = $pro_customer['firstname'] . ' ' . $pro_customer['lastname'];
        }
        
        // Buscar membership
        $stmt = $pdo->prepare("SELECT membership_level FROM om_customer_membership WHERE customer_id = ? AND status = 1");
        $stmt->execute([$pro_customer_id]);
        $pro_member = $stmt->fetch(PDO::FETCH_ASSOC);
        
        if ($pro_member) {
            $pro_membership_level = strtolower($pro_member['membership_level']);
        }
    } catch (Exception $e) {
        // Silenciar erro
    }
}

// Configurar benefÃ­cios por nÃ­vel
$pro_membership_config = [
    'bronze' => ['icon' => 'ğŸ¥‰', 'discount' => 5, 'frete' => 150, 'name' => 'Bronze'],
    'silver' => ['icon' => 'ğŸ¥ˆ', 'discount' => 10, 'frete' => 100, 'name' => 'Prata'],
    'gold' => ['icon' => 'ğŸ¥‡', 'discount' => 15, 'frete' => 50, 'name' => 'Ouro'],
    'platinum' => ['icon' => 'ğŸ’', 'discount' => 20, 'frete' => 0, 'name' => 'Platina'],
    'diamond' => ['icon' => 'ğŸ‘‘', 'discount' => 25, 'frete' => 0, 'name' => 'Diamante'],
];

if (isset($pro_membership_config[$pro_membership_level])) {
    $cfg = $pro_membership_config[$pro_membership_level];
    $pro_membership_icon = $cfg['icon'];
    $pro_membership_discount = $cfg['discount'];
    $pro_membership_frete_minimo = $cfg['frete'];
    $pro_membership_name = $cfg['name'];
}

// SaudaÃ§Ã£o por horÃ¡rio
$hora = (int)date('H');
if ($hora >= 5 && $hora < 12) {
    $pro_saudacao = 'Bom dia';
} elseif ($hora >= 12 && $hora < 18) {
    $pro_saudacao = 'Boa tarde';
} else {
    $pro_saudacao = 'Boa noite';
}

