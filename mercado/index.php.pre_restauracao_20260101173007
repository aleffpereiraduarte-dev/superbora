session_start();
// Em produÃ§Ã£o usar log de erros ao invÃ©s de suprimir
if (defined('ENVIRONMENT') && ENVIRONMENT === 'production') {
    error_reporting(E_ERROR | E_PARSE);
    ini_set('log_errors', 1);
    ini_set('display_errors', 0);
} else {
    error_reporting(E_ALL);
    ini_set('display_errors', 1);
}

// ConexÃ£o
$pdo = new PDO("mysql:host=localhost;dbname=love1;charset=utf8mb4", "love1", "hlo8LcJuND@", [
    PDO::ATTR_ERRMODE => PDO::ERRMODE_EXCEPTION,
    PDO::ATTR_DEFAULT_FETCH_MODE => PDO::FETCH_ASSOC
]);

// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
// DADOS COMPLETOS DO CLIENTE OPENCART - World Class Integration
// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

$customer_id = $_pro_customer_id ?? ($_SESSION['customer_id'] ?? 0);
$customer = null;
$customer_address = null;
$customer_addresses = [];
$membership = null;
$is_logged = false;
$customer_name = '';

if ($customer_id) {
    // Dados completos do cliente
    $stmt = $pdo->prepare("
        SELECT c.*, cg.name as customer_group_name
        FROM oc_customer c
        LEFT JOIN oc_customer_group_description cg ON c.customer_group_id = cg.customer_group_id AND cg.language_id = 2
        WHERE c.customer_id = ?
    ");
    $stmt->execute([$customer_id]);
    $customer = $stmt->fetch();
    
    if ($customer) {
        $is_logged = true;
        $customer_name = $customer['firstname'];
        
        // Buscar endereÃ§os
        $stmt = $pdo->prepare("
            SELECT a.*, z.code as zone_code
            FROM oc_address a
            LEFT JOIN oc_zone z ON a.zone_id = z.zone_id
            WHERE a.customer_id = ?
            ORDER BY a.address_id = ? DESC
        ");
        $stmt->execute([$customer_id, $customer['address_id']]);
        $customer_addresses = $stmt->fetchAll();
        
        if (!empty($customer_addresses)) {
            foreach ($customer_addresses as $addr) {
                if ($addr['address_id'] == $customer['address_id']) {
                    $customer_address = $addr;
                    break;
                }
            }
            if (!$customer_address) $customer_address = $customer_addresses[0];
        }
        
        // Membership
        try {
            $stmt = $pdo->prepare("SELECT * FROM om_membership WHERE customer_id = ? AND status = 1");
            $stmt->execute([$customer_id]);
            $membership = $stmt->fetch();
        } catch (Exception $e) {}
    }
}

// NÃ­veis de membership com frete
$membership_level = 'visitante';
$membership_discount = 0;
$membership_icon = 'ğŸ‘¤';
$membership_color = '#94a3b8';
$membership_frete_gratis = false;
$membership_frete_minimo = 150; // MÃ­nimo para frete grÃ¡tis sem membership
$membership_benefits = [];

if ($is_logged && !$membership) {
    $membership_level = 'bronze';
    $membership_icon = 'ğŸ¥‰';
    $membership_color = '#cd7f32';
    $membership_frete_minimo = 120;
    $membership_benefits = ['5% em produtos selecionados'];
}

if ($membership) {
    $level = strtolower($membership['level'] ?? 'bronze');
    $membership_level = $level;
    $levels_config = [
        'bronze' => ['discount' => 0, 'icon' => 'ğŸ¥‰', 'color' => '#cd7f32', 'frete_minimo' => 120, 'frete_gratis' => false, 'benefits' => ['5% em produtos selecionados']],
        'silver' => ['discount' => 3, 'icon' => 'ğŸ¥ˆ', 'color' => '#C0C0C0', 'frete_minimo' => 80, 'frete_gratis' => false, 'benefits' => ['3% desconto geral', 'Frete grÃ¡tis acima de R$80']],
        'prata' => ['discount' => 3, 'icon' => 'ğŸ¥ˆ', 'color' => '#C0C0C0', 'frete_minimo' => 80, 'frete_gratis' => false, 'benefits' => ['3% desconto geral', 'Frete grÃ¡tis acima de R$80']],
        'gold' => ['discount' => 5, 'icon' => 'ğŸ¥‡', 'color' => '#FFD700', 'frete_minimo' => 50, 'frete_gratis' => false, 'benefits' => ['5% desconto geral', 'Frete grÃ¡tis acima de R$50', 'Ofertas exclusivas']],
        'ouro' => ['discount' => 5, 'icon' => 'ğŸ¥‡', 'color' => '#FFD700', 'frete_minimo' => 50, 'frete_gratis' => false, 'benefits' => ['5% desconto geral', 'Frete grÃ¡tis acima de R$50', 'Ofertas exclusivas']],
        'platinum' => ['discount' => 8, 'icon' => 'ğŸ’', 'color' => '#E5E4E2', 'frete_minimo' => 0, 'frete_gratis' => true, 'benefits' => ['8% desconto geral', 'Frete SEMPRE grÃ¡tis', 'Acesso antecipado', 'Suporte prioritÃ¡rio']],
        'platina' => ['discount' => 8, 'icon' => 'ğŸ’', 'color' => '#E5E4E2', 'frete_minimo' => 0, 'frete_gratis' => true, 'benefits' => ['8% desconto geral', 'Frete SEMPRE grÃ¡tis', 'Acesso antecipado', 'Suporte prioritÃ¡rio']],
        'diamond' => ['discount' => 10, 'icon' => 'ğŸ‘‘', 'color' => '#b9f2ff', 'frete_minimo' => 0, 'frete_gratis' => true, 'benefits' => ['10% desconto geral', 'Frete SEMPRE grÃ¡tis', 'Acesso VIP', 'Suporte 24h', 'Cashback 2%']],
        'diamante' => ['discount' => 10, 'icon' => 'ğŸ‘‘', 'color' => '#b9f2ff', 'frete_minimo' => 0, 'frete_gratis' => true, 'benefits' => ['10% desconto geral', 'Frete SEMPRE grÃ¡tis', 'Acesso VIP', 'Suporte 24h', 'Cashback 2%']]
    ];
    
    if (isset($levels_config[$level])) {
        $cfg = $levels_config[$level];
        $membership_discount = $cfg['discount'];
        $membership_icon = $cfg['icon'];
        $membership_color = $cfg['color'];
        $membership_frete_minimo = $cfg['frete_minimo'];
        $membership_frete_gratis = $cfg['frete_gratis'];
        $membership_benefits = $cfg['benefits'];
    }
}

// SaudaÃ§Ã£o
$hora = (int)date('H');
if ($hora >= 5 && $hora < 12) {
    $saudacao = 'Bom dia';
    $emoji = 'â˜€ï¸';
} elseif ($hora >= 12 && $hora < 18) {
    $saudacao = 'Boa tarde';
    $emoji = 'ğŸŒ¤ï¸';
} else {
    $saudacao = 'Boa noite';
    $emoji = 'ğŸŒ™';
}

// Parceiro/Mercado - Pegar da sessÃ£o ou detectar pelo CEP do cliente
$partner_id = $_SESSION['market_partner_id'] ?? 0;

// â•â•â• FIX: VERIFICAR SE TEM MERCADO SELECIONADO â•â•â•
$tem_mercado_selecionado = isset($partner_id) && $partner_id > 0;
if (!$tem_mercado_selecionado) {
    $partner_id = 0; // Valor seguro para queries
}

if (!$partner_id && $customer_id) {
    // Tentar pegar pelo endereÃ§o do cliente
    try {
        $stmt = $pdo->prepare("SELECT postcode FROM oc_address WHERE customer_id = ? AND postcode != '' ORDER BY address_id DESC LIMIT 1");
        $stmt->execute([$customer_id]);
        $addr = $stmt->fetch();
        if ($addr && $addr['postcode']) {
            $cep = preg_replace('/\D/', '', $addr['postcode']);
            $prefixo = substr($cep, 0, 5);
            $stmt = $pdo->prepare("SELECT partner_id FROM om_market_partners WHERE status = 1 AND cep_inicio <= ? AND cep_fim >= ? ORDER BY partner_id LIMIT 1");
            $stmt->execute([$prefixo, $prefixo]);
            $p = $stmt->fetch();
            if ($p) $partner_id = $p['partner_id'];
        }
    } catch (Exception $e) {}
}

// Default: parceiro 4
if (!$partner_id) { $partner_id = null; /* Sistema usa CEP */ }
$_SESSION['market_partner_id'] = " . intval($partner_id ?: 0) . ";

$tempo_entrega = 25;

