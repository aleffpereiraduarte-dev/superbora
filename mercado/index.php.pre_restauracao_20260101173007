<!-- MERCADO V9 FIX -->
<script>
(function(){
    // Corrigir links do bottom nav
    document.querySelectorAll(".bottom-nav-item, .bottom-nav a, [href*=\"cat=ofertas\"]").forEach(function(el){
        if(el.href && el.href.includes("cat=ofertas")){
            el.href = "/mercado/ofertas/";
        }
    });
    
    // Corrigir links de ofertas em geral
    document.querySelectorAll("a[href*=\"ofertas\"], [onclick*=\"ofertas\"]").forEach(function(el){
        if(el.href && el.href.includes("?cat=ofertas")){
            el.href = "/mercado/ofertas/";
        }
        if(el.onclick){
            el.onclick = function(){ location.href = "/mercado/ofertas/"; };
        }
    });
    
    // Interceptar cliques em elementos com texto "Ofertas"
    document.addEventListener("click", function(e){
        var t = e.target.closest("a, button, .bottom-nav-item");
        if(t){
            var txt = t.textContent || "";
            var href = t.href || "";
            if(txt.includes("Ofertas") || href.includes("ofertas")){
                if(!href.includes("/mercado/ofertas/")){
                    e.preventDefault();
                    location.href = "/mercado/ofertas/";
                }
            }
        }
    });
})();
</script>
<!-- /MERCADO V9 FIX -->


<script>
// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
// ğŸš€ ONEMUNDO PREMIUM INTERACTIONS v2.0
// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

// Toast de notificaÃ§Ã£o
function showToast(message, icon = "âœ“") {
    let toast = document.querySelector(".om-toast");
    if (!toast) {
        toast = document.createElement("div");
        toast.className = "om-toast";
        document.body.appendChild(toast);
    }
    
    toast.innerHTML = `
        <div class="om-toast-icon">${icon}</div>
        <span>${message}</span>
    `;
    
    setTimeout(() => toast.classList.add("show"), 10);
    setTimeout(() => toast.classList.remove("show"), 3000);
}

// Confetti ao adicionar produto
function createConfetti(x, y) {
    const colors = ["#10b981", "#3b82f6", "#f59e0b", "#ef4444", "#8b5cf6"];
    
    for (let i = 0; i < 20; i++) {
        const confetti = document.createElement("div");
        confetti.className = "confetti";
        confetti.style.cssText = `
            left: ${x + (Math.random() - 0.5) * 100}px;
            top: ${y}px;
            background: ${colors[Math.floor(Math.random() * colors.length)]};
            transform: rotate(${Math.random() * 360}deg);
        `;
        document.body.appendChild(confetti);
        
        setTimeout(() => confetti.remove(), 1000);
    }
}

// AnimaÃ§Ã£o produto voando pro carrinho
function flyToCart(element) {
    const cart = document.querySelector(".cart-btn, .cart-fixed, [data-cart-icon]");
    if (!cart) return;
    
    const rect = element.getBoundingClientRect();
    const cartRect = cart.getBoundingClientRect();
    
    const fly = document.createElement("div");
    fly.className = "product-fly-animation";
    fly.innerHTML = element.querySelector("img")?.outerHTML || "ğŸ›’";
    fly.style.cssText = `
        position: fixed;
        left: ${rect.left + rect.width/2}px;
        top: ${rect.top + rect.height/2}px;
        width: 60px;
        height: 60px;
        border-radius: 50%;
        background: white;
        box-shadow: 0 10px 30px rgba(0,0,0,0.2);
        display: flex;
        align-items: center;
        justify-content: center;
        z-index: 10000;
    `;
    
    if (fly.querySelector("img")) {
        fly.querySelector("img").style.cssText = "width: 40px; height: 40px; object-fit: contain;";
    }
    
    document.body.appendChild(fly);
    
    requestAnimationFrame(() => {
        fly.style.left = cartRect.left + cartRect.width/2 + "px";
        fly.style.top = cartRect.top + cartRect.height/2 + "px";
        fly.style.transform = "scale(0.3)";
        fly.style.opacity = "0";
    });
    
    setTimeout(() => {
        fly.remove();
        cart.style.transform = "scale(1.2)";
        setTimeout(() => cart.style.transform = "", 200);
    }, 600);
}

// Timer flip dramÃ¡tico
function updateFlashTimer() {
    const timer = document.querySelector(".flash-premium-timer");
    if (!timer) return;
    
    const now = new Date();
    const end = new Date();
    end.setHours(23, 59, 59, 999);
    
    let diff = Math.max(0, end - now);
    
    const hours = Math.floor(diff / 3600000);
    const minutes = Math.floor((diff % 3600000) / 60000);
    const seconds = Math.floor((diff % 60000) / 1000);
    
    const hoursEl = timer.querySelector("[data-hours]");
    const minutesEl = timer.querySelector("[data-minutes]");
    const secondsEl = timer.querySelector("[data-seconds]");
    
    if (hoursEl) hoursEl.textContent = String(hours).padStart(2, "0");
    if (minutesEl) minutesEl.textContent = String(minutes).padStart(2, "0");
    if (secondsEl) secondsEl.textContent = String(seconds).padStart(2, "0");
}

setInterval(updateFlashTimer, 1000);
updateFlashTimer();

// Dark mode toggle
function toggleDarkMode() {
    const current = document.documentElement.getAttribute("data-theme");
    const newTheme = current === "dark" ? "light" : "dark";
    document.documentElement.setAttribute("data-theme", newTheme);
    localStorage.setItem("theme", newTheme);
    
    const toggle = document.querySelector(".dark-mode-toggle");
    if (toggle) {
        toggle.textContent = newTheme === "dark" ? "â˜€ï¸" : "ğŸŒ™";
    }
}

// Carregar tema salvo
document.addEventListener("DOMContentLoaded", function() {
    const savedTheme = localStorage.getItem("theme");
    if (savedTheme) {
        document.documentElement.setAttribute("data-theme", savedTheme);
    }
    
    // Adicionar botÃ£o favorito nos cards
    document.querySelectorAll(".product-card").forEach(card => {
        if (!card.querySelector(".product-favorite")) {
            const fav = document.createElement("button");
            fav.className = "product-favorite";
            fav.innerHTML = "â™¡";
            fav.onclick = function(e) {
                e.stopPropagation();
                this.classList.toggle("active");
                this.innerHTML = this.classList.contains("active") ? "â™¥" : "â™¡";
                showToast(this.classList.contains("active") ? "Adicionado aos favoritos" : "Removido dos favoritos", "â¤ï¸");
            };
            card.querySelector(".product-image")?.appendChild(fav);
        }
    });
});

// Intersection Observer para animaÃ§Ãµes on scroll
const observerOptions = {
    threshold: 0.1,
    rootMargin: "0px 0px -50px 0px"
};

const animateOnScroll = new IntersectionObserver((entries) => {
    entries.forEach(entry => {
        if (entry.isIntersecting) {
            entry.target.classList.add("animate-in");
            animateOnScroll.unobserve(entry.target);
        }
    });
}, observerOptions);

document.querySelectorAll(".product-card, .bento-item, .cesta-3d, .flash-premium-card").forEach(el => {
    el.style.opacity = "0";
    el.style.transform = "translateY(30px)";
    el.style.transition = "all 0.6s cubic-bezier(0.34, 1.56, 0.64, 1)";
    animateOnScroll.observe(el);
});

document.head.insertAdjacentHTML("beforeend", `
    <style>
        .animate-in {
            opacity: 1 !important;
            transform: translateY(0) !important;
        }
    </style>
`);

// Prova social automÃ¡tica
const provasSociais = [
    { nome: "Maria S.", cidade: "SÃ£o Paulo", produto: "Cesta BÃ¡sica", tempo: "2 min" },
    { nome: "JoÃ£o P.", cidade: "Rio de Janeiro", produto: "Kit Churrasco", tempo: "5 min" },
    { nome: "Ana C.", cidade: "Belo Horizonte", produto: "Hortifruti", tempo: "8 min" },
    { nome: "Carlos M.", cidade: "Curitiba", produto: "Bebidas", tempo: "12 min" },
];

function showProvaSocial() {
    const prova = provasSociais[Math.floor(Math.random() * provasSociais.length)];
    
    let socialEl = document.querySelector(".prova-social-popup");
    if (!socialEl) {
        socialEl = document.createElement("div");
        socialEl.className = "prova-social-popup";
        socialEl.style.cssText = `
            position: fixed;
            bottom: 24px;
            left: 24px;
            background: white;
            border-radius: 16px;
            padding: 16px 20px;
            box-shadow: 0 10px 40px rgba(0,0,0,0.15);
            z-index: 999;
            display: flex;
            align-items: center;
            gap: 14px;
            max-width: 320px;
            transform: translateX(-120%);
            transition: transform 0.5s cubic-bezier(0.34, 1.56, 0.64, 1);
        `;
        document.body.appendChild(socialEl);
    }
    
    socialEl.innerHTML = `
        <div style="width: 44px; height: 44px; background: linear-gradient(135deg, #dcfce7, #bbf7d0); border-radius: 50%; display: flex; align-items: center; justify-content: center; font-size: 18px; font-weight: 700; color: #16a34a;">
            ${prova.nome.charAt(0)}
        </div>
        <div style="flex: 1;">
            <p style="margin: 0; font-size: 14px; color: #333;">
                <strong>${prova.nome}</strong> de ${prova.cidade}
            </p>
            <p style="margin: 4px 0 0; font-size: 13px; color: #666;">
                Comprou <strong>${prova.produto}</strong>
            </p>
            <span style="font-size: 11px; color: #999;">hÃ¡ ${prova.tempo}</span>
        </div>
        <button onclick="this.parentElement.style.transform='translateX(-120%)'" style="position: absolute; top: 8px; right: 8px; background: none; border: none; color: #999; cursor: pointer; font-size: 16px;">Ã—</button>
    `;
    
    setTimeout(() => socialEl.style.transform = "translateX(0)", 100);
    setTimeout(() => socialEl.style.transform = "translateX(-120%)", 6000);
}

// Mostrar prova social a cada 30 segundos
setTimeout(showProvaSocial, 5000);
setInterval(showProvaSocial, 30000);

console.log("ğŸ¨ OneMundo Premium Layout v2.0 carregado!");
</script>

<!-- ğŸ–¼ï¸ Sistema de Imagens Premium -->

<script>
// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
// ğŸ–¼ï¸ ONEMUNDO - SISTEMA DE IMAGENS PREMIUM v1.0
// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

(function() {
    "use strict";
    
    // Mapeamento de categorias para Ã­cones placeholder
    const categoryIcons = {
        "hortifruti": "ğŸ¥¬",
        "frutas": "ğŸ",
        "verduras": "ğŸ¥¦",
        "legumes": "ğŸ¥•",
        "acougue": "ğŸ¥©",
        "carnes": "ğŸ–",
        "padaria": "ğŸ¥–",
        "paes": "ğŸ",
        "bebidas": "ğŸ¥¤",
        "refrigerantes": "ğŸ¥¤",
        "sucos": "ğŸ§ƒ",
        "cervejas": "ğŸº",
        "vinhos": "ğŸ·",
        "laticinios": "ğŸ¥›",
        "leite": "ğŸ¥›",
        "queijos": "ğŸ§€",
        "iogurtes": "ğŸ¥›",
        "limpeza": "ğŸ§¹",
        "higiene": "ğŸ§´",
        "congelados": "ğŸ§Š",
        "mercearia": "ğŸ›’",
        "graos": "ğŸŒ¾",
        "cereais": "ğŸ¥£",
        "massas": "ğŸ",
        "doces": "ğŸ«",
        "biscoitos": "ğŸª",
        "pet": "ğŸ•",
        "bebe": "ğŸ‘¶",
        "default": "ğŸ“¦"
    };
    
    // Cores por categoria
    const categoryColors = {
        "hortifruti": { bg: "linear-gradient(145deg, #f0fdf4, #dcfce7)", color: "#059669" },
        "frutas": { bg: "linear-gradient(145deg, #fff7ed, #fed7aa)", color: "#ea580c" },
        "acougue": { bg: "linear-gradient(145deg, #fef2f2, #fecaca)", color: "#dc2626" },
        "carnes": { bg: "linear-gradient(145deg, #fef2f2, #fecaca)", color: "#dc2626" },
        "padaria": { bg: "linear-gradient(145deg, #fffbeb, #fde68a)", color: "#d97706" },
        "bebidas": { bg: "linear-gradient(145deg, #eff6ff, #bfdbfe)", color: "#2563eb" },
        "laticinios": { bg: "linear-gradient(145deg, #f0f9ff, #bae6fd)", color: "#0284c7" },
        "limpeza": { bg: "linear-gradient(145deg, #ecfeff, #a5f3fc)", color: "#0891b2" },
        "higiene": { bg: "linear-gradient(145deg, #fdf4ff, #f5d0fe)", color: "#a855f7" },
        "congelados": { bg: "linear-gradient(145deg, #f0f9ff, #e0f2fe)", color: "#0ea5e9" },
        "default": { bg: "linear-gradient(145deg, #f8fafc, #e2e8f0)", color: "#64748b" }
    };
    
    // Detectar categoria pelo nome do produto
    function detectCategory(productName, brandName) {
        const text = (productName + " " + (brandName || "")).toLowerCase();
        
        if (/carne|boi|frango|porco|costela|picanha|alcatra|file|linguica|salsicha|bacon|presunto|mortadela/.test(text)) return "carnes";
        if (/leite|queijo|iogurte|manteiga|requeijao|cream cheese|nata|coalhada/.test(text)) return "laticinios";
        if (/pao|bolo|torta|biscoito|bolacha|croissant|rosca/.test(text)) return "padaria";
        if (/refrigerante|coca|pepsi|guarana|fanta|sprite|suco|agua|cerveja|vinho|whisky|vodka/.test(text)) return "bebidas";
        if (/alface|tomate|cebola|batata|cenoura|banana|maca|laranja|limao|abacaxi|melancia|uva|morango/.test(text)) return "hortifruti";
        if (/sabao|detergente|desinfetante|agua sanitaria|alvejante|limpador|esponja/.test(text)) return "limpeza";
        if (/shampoo|condicionador|sabonete|creme|desodorante|papel higienico|absorvente/.test(text)) return "higiene";
        if (/sorvete|pizza|lasanha|hamburguer|nuggets|congelado/.test(text)) return "congelados";
        if (/arroz|feijao|macarrao|oleo|acucar|sal|farinha|cafe|cha/.test(text)) return "mercearia";
        if (/racao|petisco|areia|cachorro|gato/.test(text)) return "pet";
        
        return "default";
    }
    
    // Criar placeholder premium
    function createPlaceholder(productName, brandName) {
        const category = detectCategory(productName, brandName);
        const icon = categoryIcons[category] || categoryIcons.default;
        const colors = categoryColors[category] || categoryColors.default;
        
        const placeholder = document.createElement("div");
        placeholder.className = "product-placeholder";
        placeholder.style.background = colors.bg;
        placeholder.innerHTML = `
            <span class="product-placeholder-icon">${icon}</span>
            <span class="product-placeholder-text" style="color: ${colors.color}">Imagem em breve</span>
        `;
        
        return placeholder;
    }
    
    // Processar imagens dos produtos
    function processProductImages() {
        const productCards = document.querySelectorAll(".product-card");
        
        productCards.forEach((card, index) => {
            const imageContainer = card.querySelector(".product-image");
            const img = imageContainer?.querySelector("img");
            const nameEl = card.querySelector(".product-name");
            const brandEl = card.querySelector(".product-brand");
            
            if (!imageContainer) return;
            
            const productName = nameEl?.textContent || "";
            const brandName = brandEl?.textContent || "";
            
            // Adicionar loading state
            imageContainer.classList.add("loading");
            
            if (img) {
                // Handler para quando a imagem carrega
                img.onload = function() {
                    imageContainer.classList.remove("loading");
                    
                    // Verificar se Ã© uma imagem vÃ¡lida (nÃ£o placeholder genÃ©rico)
                    if (this.naturalWidth < 50 || this.naturalHeight < 50) {
                        this.style.display = "none";
                        const placeholder = createPlaceholder(productName, brandName);
                        imageContainer.appendChild(placeholder);
                    }
                };
                
                // Handler para erro de imagem
                img.onerror = function() {
                    imageContainer.classList.remove("loading");
                    this.style.display = "none";
                    
                    const placeholder = createPlaceholder(productName, brandName);
                    imageContainer.appendChild(placeholder);
                };
                
                // Se a imagem jÃ¡ carregou (cache)
                if (img.complete) {
                    imageContainer.classList.remove("loading");
                    
                    if (img.naturalWidth < 50 || !img.src || img.src.includes("placeholder")) {
                        img.style.display = "none";
                        const placeholder = createPlaceholder(productName, brandName);
                        imageContainer.appendChild(placeholder);
                    }
                }
            } else {
                // Sem imagem, criar placeholder
                imageContainer.classList.remove("loading");
                const placeholder = createPlaceholder(productName, brandName);
                imageContainer.appendChild(placeholder);
            }
            
            // Adicionar botÃ£o de favorito se nÃ£o existir
            if (!card.querySelector(".product-favorite")) {
                const favBtn = document.createElement("button");
                favBtn.className = "product-favorite";
                favBtn.innerHTML = "â™¡";
                favBtn.setAttribute("aria-label", "Adicionar aos favoritos");
                
                favBtn.onclick = function(e) {
                    e.stopPropagation();
                    e.preventDefault();
                    
                    this.classList.add("animating");
                    this.classList.toggle("active");
                    this.innerHTML = this.classList.contains("active") ? "â™¥" : "â™¡";
                    
                    // Feedback visual
                    showToast(
                        this.classList.contains("active") 
                            ? "Adicionado aos favoritos â¤ï¸" 
                            : "Removido dos favoritos",
                        this.classList.contains("active") ? "â¤ï¸" : "ğŸ’”"
                    );
                    
                    setTimeout(() => this.classList.remove("animating"), 600);
                };
                
                imageContainer.appendChild(favBtn);
            }
            
            // AnimaÃ§Ã£o de entrada escalonada
            card.style.animationDelay = `${index * 0.05}s`;
        });
    }
    
    // Processar ofertas relÃ¢mpago
    function processFlashCards() {
        const flashCards = document.querySelectorAll(".flash-card");
        
        flashCards.forEach(card => {
            const img = card.querySelector(".flash-image img");
            const nameEl = card.querySelector(".flash-name");
            const brandEl = card.querySelector(".flash-brand");
            
            if (img) {
                img.onerror = function() {
                    const category = detectCategory(
                        nameEl?.textContent || "",
                        brandEl?.textContent || ""
                    );
                    const icon = categoryIcons[category] || categoryIcons.default;
                    
                    this.style.display = "none";
                    this.parentElement.innerHTML = `
                        <div style="font-size: 64px; animation: cesta-bounce 3s ease-in-out infinite;">
                            ${icon}
                        </div>
                    `;
                };
            }
        });
    }
    
    // Adicionar efeito ripple nos botÃµes
    function addRippleEffect() {
        document.querySelectorAll(".add-btn").forEach(btn => {
            btn.addEventListener("click", function(e) {
                const rect = this.getBoundingClientRect();
                const ripple = document.createElement("span");
                ripple.className = "ripple";
                ripple.style.left = (e.clientX - rect.left) + "px";
                ripple.style.top = (e.clientY - rect.top) + "px";
                this.appendChild(ripple);
                
                setTimeout(() => ripple.remove(), 600);
            });
        });
    }
    
    // Lazy loading melhorado
    function setupLazyLoading() {
        if ("IntersectionObserver" in window) {
            const imageObserver = new IntersectionObserver((entries, observer) => {
                entries.forEach(entry => {
                    if (entry.isIntersecting) {
                        const img = entry.target;
                        if (img.dataset.src) {
                            img.src = img.dataset.src;
                            img.removeAttribute("data-src");
                        }
                        observer.unobserve(img);
                    }
                });
            }, {
                rootMargin: "100px"
            });
            
            document.querySelectorAll("img[data-src]").forEach(img => {
                imageObserver.observe(img);
            });
        }
    }
    
    // Inicializar quando DOM estiver pronto
    function init() {
        processProductImages();
        processFlashCards();
        addRippleEffect();
        setupLazyLoading();
        
        console.log("ğŸ–¼ï¸ Sistema de imagens premium carregado!");
    }
    
    // Executar
    if (document.readyState === "loading") {
        document.addEventListener("DOMContentLoaded", init);
    } else {
        init();
    }
    
    // Re-processar quando novos produtos forem adicionados (para paginaÃ§Ã£o AJAX)
    window.reprocessImages = processProductImages;
    
})();
</script>

<!-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
     ğŸ›’ ONEMUNDO MERCADO - MODAL PRODUTO ESTILO INSTACART PERFEITO
     Todas as funcionalidades do Instacart implementadas
     â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• -->

