<script>
(function(){
"use strict";

// Config
var OM_ENDERECO_ATUAL = null;
var OM_ENDERECOS = [];

// Carregar endereÃ§os via AJAX
function omCarregarEnderecos() {
    fetch('/mercado/api/address.php?action=list')
    .then(function(r){return r.json()})
    .then(function(data){
        if(data.addresses){
            OM_ENDERECOS = data.addresses;
            OM_ENDERECO_ATUAL = data.current;
            omAtualizarTopbar();
            omRenderEnderecos();
        }
    })
    .catch(function(){
        // Fallback - pegar do PHP se disponÃ­vel
        var el = document.getElementById('omEnderecoData');
        if(el){
            try {
                var d = JSON.parse(el.textContent);
                OM_ENDERECO_ATUAL = d.current;
                OM_ENDERECOS = d.addresses || [];
                omAtualizarTopbar();
                omRenderEnderecos();
            }catch(e){}
        }
    });
}

// Atualizar topbar com endereÃ§o
function omAtualizarTopbar() {
    var el = document.getElementById('omEnderecoTexto');
    if(el && OM_ENDERECO_ATUAL){
        var txt = OM_ENDERECO_ATUAL.address_1 || OM_ENDERECO_ATUAL.street || 'Selecione o endereÃ§o';
        if(OM_ENDERECO_ATUAL.number) txt += ', ' + OM_ENDERECO_ATUAL.number;
        el.textContent = txt;
    }
}

// Renderizar lista de endereÃ§os no modal
function omRenderEnderecos() {
    var container = document.getElementById('omAddrList');
    if(!container) return;
    
    var html = '';
    OM_ENDERECOS.forEach(function(addr, i){
        var isSelected = OM_ENDERECO_ATUAL && (addr.address_id == OM_ENDERECO_ATUAL.address_id);
        var tipo = addr.tipo || (i === 0 ? 'Casa' : 'EndereÃ§o ' + (i+1));
        var rua = addr.address_1 || addr.street || 'EndereÃ§o';
        var numero = addr.number || '';
        var bairro = addr.address_2 || addr.neighborhood || '';
        var cidade = addr.city || '';
        
        html += '<div class="om-addr-card'+(isSelected?' selected':'')+'" onclick="omSelectAddr('+addr.address_id+')">';
        html += '<div class="om-addr-icon"><svg fill="none" stroke="currentColor" viewBox="0 0 24 24"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17.657 16.657L13.414 20.9a1.998 1.998 0 01-2.827 0l-4.244-4.243a8 8 0 1111.314 0z"/><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 11a3 3 0 11-6 0 3 3 0 016 0z"/></svg></div>';
        html += '<div class="om-addr-info">';
        html += '<div class="om-addr-type">'+tipo+'</div>';
        html += '<div class="om-addr-street">'+rua+(numero?', '+numero:'')+'</div>';
        html += '<div class="om-addr-details">'+bairro+(cidade?' â€¢ '+cidade:'')+'</div>';
        html += '</div>';
        html += '<div class="om-addr-check"><svg fill="none" stroke="currentColor" viewBox="0 0 24 24"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="3" d="M5 13l4 4L19 7"/></svg></div>';
        html += '</div>';
    });
    
    html += '<button class="om-new-addr" onclick="omNovoEndereco()">';
    html += '<svg fill="none" stroke="currentColor" viewBox="0 0 24 24"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 4v16m8-8H4"/></svg>';
    html += 'Adicionar novo endereÃ§o';
    html += '</button>';
    
    container.innerHTML = html;
}

// Selecionar endereÃ§o
window.omSelectAddr = function(id) {
    fetch('/mercado/api/address.php', {
        method: 'POST',
        headers: {'Content-Type': 'application/json'},
        body: JSON.stringify({action: 'select', address_id: id})
    })
    .then(function(r){return r.json()})
    .then(function(data){
        if(data.success){
            location.reload();
        }
    });
};

// Novo endereÃ§o
window.omNovoEndereco = function() {
    window.location.href = '/index.php?route=account/address/add';
};

// Abrir modal
window.omOpenModal = function() {
    document.getElementById('omModalOverlay').classList.add('open');
    document.body.style.overflow = 'hidden';
};

// Fechar modal
window.omCloseModal = function() {
    document.getElementById('omModalOverlay').classList.remove('open');
    document.body.style.overflow = '';
};

// Fix carrinho
window.toggleCart = function(){window.location.href='/mercado/carrinho/'};
window.openCartSidebar = function(){window.location.href='/mercado/carrinho/'};

document.addEventListener('click', function(e){
    var t = e.target.closest('.mkt-cart,[onclick*="toggleCart"],[onclick*="openCartSidebar"],.mobile-nav-cart');
    if(t){e.preventDefault();e.stopPropagation();window.location.href='/mercado/carrinho/';return false}
});

document.querySelectorAll('.pro-bottom-nav-item,.mobile-nav-item').forEach(function(el){
    if(el.textContent.indexOf('Carrinho')>-1 || el.innerHTML.indexOf('ğŸ›’')>-1){
        el.onclick = function(e){e.preventDefault();window.location.href='/mercado/carrinho/'};
    }
});

// Criar topbar
var h = document.querySelector('.mkt-header');
if(h && !document.querySelector('.om-topbar')){
    var t = document.createElement('div');
    t.className = 'om-topbar';
    t.onclick = omOpenModal;
    t.innerHTML = '<div class="om-topbar-main"><div class="om-topbar-icon"><svg fill="none" stroke="currentColor" viewBox="0 0 24 24"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17.657 16.657L13.414 20.9a1.998 1.998 0 01-2.827 0l-4.244-4.243a8 8 0 1111.314 0z"/><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 11a3 3 0 11-6 0 3 3 0 016 0z"/></svg></div><div class="om-topbar-content"><div class="om-topbar-label"><span>Entregar em</span></div><div class="om-topbar-address" id="omEnderecoTexto">Carregando...</div></div><div class="om-topbar-arrow"><svg fill="none" stroke="currentColor" viewBox="0 0 24 24"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7"/></svg></div></div><div class="om-topbar-time"><svg fill="none" stroke="currentColor" viewBox="0 0 24 24"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 8v4l3 3m6-3a9 9 0 11-18 0 9 9 0 0118 0z"/></svg>25-35 min</div>';
    h.insertBefore(t, h.firstChild);
}

// Logo texto
var l = document.querySelector('.mkt-logo');
if(l && !document.querySelector('.mkt-logo-text')){
    var s = document.createElement('span');
    s.className = 'mkt-logo-text';
    s.textContent = 'Mercado';
    l.appendChild(s);
}

// Iniciar
omCarregarEnderecos();
console.log('âœ… OneMundo V6 Ultra OK');
})();
</script>
<!-- ONEMUNDO INJECT V6 ULTRA - FIM -->


