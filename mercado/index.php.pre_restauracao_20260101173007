<script>
// Carrossel
let proSlide = 0;
const proTrack = document.getElementById('proCarouselTrack');
const proDots = document.querySelectorAll('.pro-carousel-dot');
const proTotal = proDots.length;

function proGoSlide(i) {
    if (i < 0) i = proTotal - 1;
    if (i >= proTotal) i = 0;
    proSlide = i;
    if (proTrack) proTrack.style.transform = 'translateX(-' + (proSlide * 100) + '%)';
    proDots.forEach((d, x) => d.classList.toggle('active', x === proSlide));
}
function proCarouselNext() { proGoSlide(proSlide + 1); }
function proCarouselPrev() { proGoSlide(proSlide - 1); }
proDots.forEach((d, i) => d.addEventListener('click', () => proGoSlide(i)));
setInterval(proCarouselNext, 5000);

// Tema
function proToggleTheme() {
    const dark = document.documentElement.getAttribute('data-theme') === 'dark';
    document.documentElement.setAttribute('data-theme', dark ? '' : 'dark');
    document.getElementById('proThemeBtn').textContent = dark ? 'ğŸŒ™' : 'â˜€ï¸';
    localStorage.setItem('pro_theme', dark ? '' : 'dark');
}
if (localStorage.getItem('pro_theme') === 'dark') {
    document.documentElement.setAttribute('data-theme', 'dark');
    document.getElementById('proThemeBtn').textContent = 'â˜€ï¸';
}

// Cupom
function proCopyCupom(code) {
    navigator.clipboard.writeText(code).then(() => proShowToast('âœ…', 'Cupom copiado!', code)).catch(() => {
        const i = document.createElement('input'); i.value = code; document.body.appendChild(i);
        i.select(); document.execCommand('copy'); document.body.removeChild(i);
        proShowToast('âœ…', 'Cupom copiado!', code);
    });
}

// Toast
function proShowToast(icon, title, text) {
    document.getElementById('proToastIcon').textContent = icon;
    document.getElementById('proToastTitle').textContent = title;
    document.getElementById('proToastText').textContent = text;
    document.getElementById('proToast').classList.add('show');
    setTimeout(() => document.getElementById('proToast').classList.remove('show'), 3500);
}

// Prova Social
const proSocialData = <?= json_encode($pro_social ?? [['nome'=>'Maria S.','cidade'=>'SÃ£o Paulo','produto'=>'Cesta BÃ¡sica','tempo'=>'2 min']]) ?>;
let proSocialIdx = 0;
function proShowSocial() {
    if (!proSocialData.length) return;
    const d = proSocialData[proSocialIdx];
    const avatar = document.getElementById('proSocialAvatar');
    const name = document.getElementById('proSocialName');
    const city = document.getElementById('proSocialCity');
    const prod = document.getElementById('proSocialProd');
    const time = document.getElementById('proSocialTime');
    const social = document.getElementById('proSocial');
    if (!social) return;
    if (avatar) avatar.textContent = d.nome.charAt(0);
    if (name) name.textContent = d.nome;
    if (city) city.textContent = d.cidade;
    if (prod) prod.textContent = d.produto;
    if (time) time.textContent = d.tempo;
    social.classList.add('show');
    setTimeout(() => { social.classList.remove('show'); proSocialIdx = (proSocialIdx + 1) % proSocialData.length; }, 5000);
}
setTimeout(proShowSocial, 12000);
setInterval(proShowSocial, 35000);

// Badge carrinho
function proUpdateBadge() {
    try {
        const c = (typeof cart !== 'undefined' && Array.isArray(cart)) ? cart.reduce((a, b) => a + (b.qty || 0), 0) : 0;
        const el = document.getElementById('proCartBadge');
        if (el) el.textContent = c;
    } catch(e) {}
}
setInterval(proUpdateBadge, 1000);

// Header scroll effect
const header = document.querySelector('.om-header');
if (header) {
    window.addEventListener('scroll', () => {
        if (window.scrollY > 20) {
            header.classList.add('scrolled');
        } else {
            header.classList.remove('scrolled');
        }
    }, { passive: true });
}
</script>



