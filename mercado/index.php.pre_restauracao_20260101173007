// Buscar categorias - mais categorias
$categorias = $pdo->query("
    SELECT c.category_id, c.name, c.icon, COUNT(p.product_id) as total
    FROM om_market_categories c
    LEFT JOIN om_market_products_base p ON p.category_id = c.category_id
    WHERE c.status = 1
    GROUP BY c.category_id, c.name
    HAVING total > 0
    ORDER BY c.sort_order ASC, total DESC
    LIMIT 20
")->fetchAll();

// Filtros
$cat_id = isset($_GET['cat']) ? (int)$_GET['cat'] : 0;
$busca = isset($_GET['q']) ? trim($_GET['q']) : '';
$page = isset($_GET['page']) ? max(1, (int)$_GET['page']) : 1;
$limit = 24;
$offset = ($page - 1) * $limit;

// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
// BUSCA INTELIGENTE
// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

$termos_busca = [];
$ai_interpretou = false;

if ($busca) {
    // Limpar a busca
    $busca_limpa = trim(preg_replace('/\s+/', ' ', $busca));
    
    // Tentar cache primeiro
    $cache_key = md5(strtolower($busca_limpa));
    try {
        $pdo->exec("CREATE TABLE IF NOT EXISTS om_busca_cache (
            id INT AUTO_INCREMENT PRIMARY KEY,
            cache_key VARCHAR(32) UNIQUE,
            query_original VARCHAR(255),
            termos JSON,
            created_at DATETIME DEFAULT CURRENT_TIMESTAMP
        )");
        
        $stmt = $pdo->prepare("SELECT termos FROM om_busca_cache WHERE cache_key = ? AND created_at > DATE_SUB(NOW(), INTERVAL 7 DAY)");
        $stmt->execute([$cache_key]);
        $cached = $stmt->fetch();
        
        if ($cached && $cached['termos']) {
            $termos_busca = json_decode($cached['termos'], true) ?: [];
        }
    } catch (Exception $e) {}
    
    // Se nÃ£o tem cache, tentar AI (com timeout curto)
    if (empty($termos_busca)) {
        $termos_busca = interpretarBuscaComAI($busca_limpa);
        $ai_interpretou = true;
        
        // Salvar no cache se AI funcionou
        if (!empty($termos_busca) && $termos_busca !== [$busca_limpa]) {
            try {
                $stmt = $pdo->prepare("INSERT INTO om_busca_cache (cache_key, query_original, termos) VALUES (?, ?, ?) ON DUPLICATE KEY UPDATE termos = VALUES(termos), created_at = NOW()");
                $stmt->execute([$cache_key, $busca_limpa, json_encode($termos_busca)]);
            } catch (Exception $e) {}
        }
    }
    
    // FALLBACK: Se ainda vazio, usar busca original dividida em palavras
    if (empty($termos_busca)) {
        $termos_busca = array_filter(explode(' ', $busca_limpa), fn($t) => strlen($t) >= 2);
        if (empty($termos_busca)) {
            $termos_busca = [$busca_limpa];
        }
    }
}

function interpretarBuscaComAI($query) {
    // Groq primeiro (grÃ¡tis e rÃ¡pido)
    $ch = curl_init('https://api.groq.com/openai/v1/chat/completions');
    $payload = [
        'model' => 'llama-3.3-70b-versatile',
        'messages' => [
            ['role' => 'system', 'content' => 'VocÃª interpreta buscas de supermercado brasileiro. Retorne APENAS JSON vÃ¡lido: {"termos":["termo1","termo2"]}. Corrija erros de digitaÃ§Ã£o, expanda abreviaÃ§Ãµes (refri=refrigerante, refri coca=refrigerante coca-cola). Se for algo para fazer receita, liste os ingredientes principais.'],
            ['role' => 'user', 'content' => "Cliente buscou: \"$query\""]
        ],
        'max_tokens' => 150,
        'temperature' => 0.1
    ];
    
    curl_setopt_array($ch, [
        CURLOPT_RETURNTRANSFER => true,
        CURLOPT_POST => true,
        CURLOPT_HTTPHEADER => [
            'Content-Type: application/json',
            'Authorization: Bearer ' . getenv('GROQ_API_KEY')
        ],
        CURLOPT_POSTFIELDS => json_encode($payload),
        CURLOPT_TIMEOUT => 5
    ]);
    
    $response = curl_exec($ch);
    $code = curl_getinfo($ch, CURLINFO_HTTP_CODE);
    curl_close($ch);
    
    if ($code === 200) {
        $data = json_decode($response, true);
        $content = $data['choices'][0]['message']['content'] ?? '';
        $content = preg_replace('/```json\s*|\s*```/', '', $content);
        $parsed = json_decode(trim($content), true);
        
        if (isset($parsed['termos']) && is_array($parsed['termos']) && !empty($parsed['termos'])) {
            return $parsed['termos'];
        }
    }
    
    // Fallback: usar busca original
    return [$query];
}

// Query base
$where = "WHERE pp.partner_id = ? AND pp.status = 1 AND pp.price > 0";
$params = [$partner_id];

if ($cat_id > 0) {
    $where .= " AND pb.category_id = ?";
    $params[] = $cat_id;
}

// Busca com termos interpretados
if ($busca && !empty($termos_busca)) {
    $search_parts = [];
    foreach ($termos_busca as $termo) {
        $termo = trim($termo);
        if (strlen($termo) >= 2) {
            $search_parts[] = "(pb.name LIKE ? OR pb.brand LIKE ?)";
            $params[] = "%$termo%";
            $params[] = "%$termo%";
        }
    }
    if (!empty($search_parts)) {
        $where .= " AND (" . implode(" OR ", $search_parts) . ")";
    }
}

// NOVO: Filtro sÃ³ produtos com info completa (ingredientes ou nutriÃ§Ã£o)
if (isset($_GET['info_completa']) && $_GET['info_completa'] == '1') {
    $where .= " AND (pb.ingredients IS NOT NULL AND pb.ingredients != '' OR pb.nutrition_json IS NOT NULL)";
}

// Total
$countSql = "SELECT COUNT(DISTINCT pb.product_id) FROM om_market_products_base pb 
             JOIN om_market_products_price pp ON pb.product_id = pp.product_id $where";
$countStmt = $pdo->prepare($countSql);
$countStmt->execute($params);
$total = $countStmt->fetchColumn();
$totalPages = ceil($total / $limit);

// NOVO: Contar produtos com informaÃ§Ã£o completa (ingredientes ou nutriÃ§Ã£o)
$countCompleteSql = "SELECT COUNT(DISTINCT pb.product_id) FROM om_market_products_base pb 
             JOIN om_market_products_price pp ON pb.product_id = pp.product_id 
             $where AND (pb.ingredients IS NOT NULL AND pb.ingredients != '' OR pb.nutrition_json IS NOT NULL)";
$countCompleteStmt = $pdo->prepare($countCompleteSql);
$countCompleteStmt->execute($params);
$totalComplete = $countCompleteStmt->fetchColumn();

// Produtos - MELHORADO: Inclui ingredientes e nutriÃ§Ã£o para badge "Info Completa"
$sql = "
    SELECT pb.product_id, pb.name, pb.brand, pb.image, pb.unit,
           pb.description, pb.ingredients, pb.nutrition_json,
           pp.price, pp.price_promo, pp.stock,
           CASE WHEN pb.ingredients IS NOT NULL AND pb.ingredients != '' THEN 1 ELSE 0 END as has_ingredients,
           CASE WHEN pb.nutrition_json IS NOT NULL THEN 1 ELSE 0 END as has_nutrition
    FROM om_market_products_base pb
    JOIN om_market_products_price pp ON pb.product_id = pp.product_id
    $where
    ORDER BY pb.name
    LIMIT $limit OFFSET $offset
";
$stmt = $pdo->prepare($sql);
$stmt->execute($params);
$produtos = $stmt->fetchAll();

// Ofertas RelÃ¢mpago (com promoÃ§Ã£o) - MELHORADO
$ofertas = $pdo->query("
    SELECT pb.product_id, pb.name, pb.brand, pb.image, pb.ingredients, pb.nutrition_json,
           pp.price, pp.price_promo,
           ROUND((1 - pp.price_promo / pp.price) * 100) as desconto,
           CASE WHEN pb.ingredients IS NOT NULL AND pb.ingredients != '' THEN 1 ELSE 0 END as has_ingredients
    FROM om_market_products_base pb
    JOIN om_market_products_price pp ON pb.product_id = pp.product_id
    WHERE pp.partner_id = " . intval($partner_id ?: 0) . " AND pp.price_promo > 0 AND pp.price_promo < pp.price AND pp.status = 1
    ORDER BY desconto DESC
    LIMIT 10
")->fetchAll();

// Populares (mais vendidos ou aleatÃ³rio)
$populares = $pdo->query("
    SELECT pb.product_id, pb.name, pb.brand, pb.image, pp.price, pp.price_promo
    FROM om_market_products_base pb
    JOIN om_market_products_price pp ON pb.product_id = pp.product_id
    WHERE pp.partner_id = " . intval($partner_id ?: 0) . " AND pp.status = 1 AND pp.price > 0
    ORDER BY RAND()
    LIMIT 6
")->fetchAll();

// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
// NOVOS RECURSOS - Cestas, Mais Vendidos, Por Categoria
// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

// Mais Vendidos (simulado por enquanto - pode integrar com vendas reais depois)
$mais_vendidos = $pdo->query("
