window.MERCADO_PARTNER_ID = <?= isset($_SESSION['mercado_partner_id']) ? intval($_SESSION['mercado_partner_id']) : 0 ?>;
window.MERCADO_PARTNER_NAME = "<?= htmlspecialchars($_SESSION['mercado_partner_name'] ?? 'OneMundo Mercado') ?>";
