// STREAK REAL
$_cid = $customer_id ?? $_pro_customer_id ?? 0;
$_today = date("Y-m-d");
$feat_streak = 0;
if ($_cid && isset($pdo)) {
    try {
        $stmt = $pdo->prepare("SELECT current_streak, last_purchase_date FROM om_customer_streak WHERE customer_id = ?");
        $stmt->execute([$_cid]);
        $s = $stmt->fetch(PDO::FETCH_ASSOC);
        if ($s) {
            $yesterday = date("Y-m-d", strtotime("-1 day"));
            if ($s["last_purchase_date"] === $_today || $s["last_purchase_date"] === $yesterday) {
                $feat_streak = (int)$s["current_streak"];
            }
        }
    } catch (Exception $e) {}
}
// PONTOS REAL
$feat_points = 0;
if ($_cid && isset($pdo)) {
    try {
        $stmt = $pdo->prepare("SELECT points FROM om_customer_points WHERE customer_id = ?");
        $stmt->execute([$_cid]);
        $feat_points = (int)$stmt->fetchColumn();
    } catch (Exception $e) {}
