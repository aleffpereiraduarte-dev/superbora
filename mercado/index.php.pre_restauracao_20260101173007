// FunÃ§Ã£o para obter SVG da categoria
function getCategorySvg($name, $cat_svgs) {
    foreach ($cat_svgs as $key => $svg) {
        if (stripos($name, $key) !== false) return $svg;
    }
    return $cat_svgs['default'];
}

// FunÃ§Ã£o para obter cores da categoria
function getCategoryColors($name, $cat_colors) {
    foreach ($cat_colors as $key => $colors) {
        if (stripos($name, $key) !== false) return $colors;
    }
    return $cat_colors['default'];
}

// Categorias com Ã­cones (fallback emoji)
$cat_icons = [
    'Hortifruti' => ['ğŸ¥¬', '#22c55e'],
    'AÃ§ougue' => ['ğŸ¥©', '#ef4444'],
    'Padaria' => ['ğŸ¥–', '#f59e0b'],
    'LaticÃ­nios' => ['ğŸ¥›', '#3b82f6'],
    'Bebidas' => ['ğŸº', '#8b5cf6'],
    'Limpeza' => ['ğŸ§¹', '#06b6d4'],
